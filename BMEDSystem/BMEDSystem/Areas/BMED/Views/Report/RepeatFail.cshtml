@model IEnumerable<EDIS.Models.RepeatFailVModel>
@using Newtonsoft.Json
@{
    Layout = null;
    var qry = JsonConvert.DeserializeObject<ReportQryVModel>(TempData["qry"].ToString());
}

<p>
    <a class="btn btn-default" href="@Url.Action("ExcelRF", qry)">匯出Excel</a>
</p>
<table class="table">
    <tr>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.AssetNo)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.AssetNam)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.DocId)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.ApplyDate)
        </th>
        <th style="text-align: right">
            @Html.DisplayNameFor(model => model.AccDptNo)
        </th>
        <th style="text-align: right">
            @Html.DisplayNameFor(model => model.AccDptNam)
        </th>
        <th style="width: 100px;">
            @Html.DisplayNameFor(model => model.TroubleDes)
        </th>
        <th style="width: 100px;">
            @Html.DisplayNameFor(model => model.FailFactor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DealDes)
        </th>
        <th style="width: 100px; text-align: right">
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="text-align: right">
                @item.AssetNo 
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.DocId)
            </td>
            <td style="text-align: right">
                @item.ApplyDate.ToString("yyyy/MM/dd")
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.AccDptNo)
            </td>
            <td style="text-align: right">
                @Html.DisplayFor(modelItem => item.AccDptNam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TroubleDes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FailFactor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DealDes)
            </td>
            <td>
                @item.EndDate.ToString("yyyy/MM/dd")
            </td>
        </tr>
    }

</table>
