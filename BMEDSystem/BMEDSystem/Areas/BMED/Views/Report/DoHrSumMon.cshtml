@model IEnumerable<EDIS.Models.DoHrSumMonVModel>

@using Newtonsoft.Json

@{
    Layout = null;
    var qry = JsonConvert.DeserializeObject<ReportQryVModel>(TempData["qry"].ToString());
}
<script src="~/js/HtmltoExcel.js"></script>

<p>
    <a class="btn btn-default" href="@Url.Action("DoHrSumMonExcel", qry)">匯出Excel</a>
</p>
@*<input id="exportExcel" type="button" value="匯出Excel" class="btn btn-default"
    onclick="javascript:exportExcel('tbToExcel')" />*@
<table class="table" id="tbToExcel">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserNam)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Hours)
            </th>*@
        <th>
            @Html.DisplayNameFor(model => model.HoursR)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoursK)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Cases)
            </th>*@
        <th>
            @Html.DisplayNameFor(model => model.RCases)
        </th>
        <th>
