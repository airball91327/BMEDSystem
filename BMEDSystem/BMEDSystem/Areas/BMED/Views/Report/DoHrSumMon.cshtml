                @Html.DisplayFor(modelItem => item.OverFiveRate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OverFiveRateHigh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SelfRate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Case3M)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fail3MRate)
            </td>
            <td>
