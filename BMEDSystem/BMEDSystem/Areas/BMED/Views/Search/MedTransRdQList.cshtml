@model IEnumerable<EDIS.Models.MedTransRd>
@using System.Security.Claims
@using Microsoft.AspNetCore.Identity
@*@using EDIS.Models.Identity*@

@{
    Layout = null;
}
<style>
    td a {
        color: deepskyblue;
    }
</style>
@if (Model.Count() <= 0)
{
<p class="text-danger">無任何資料!</p>

}
else
{
<table class="table" style="width: 100%">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DOC_TYP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DOCID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.APPLYDPT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ASSETNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PLANT_NAM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STATUS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TF_EMPNAM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TF_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RD_EMPNAM)
        </th>
    </tr>
    @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DOC_TYP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DOCID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.APPLYDPT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ASSETNO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PLANT_NAM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STATUS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TF_EMPNAM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TF_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RD_EMPNAM)
        </td>
    </tr>
        }

</table>
}
